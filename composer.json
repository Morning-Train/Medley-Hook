{
    "name": "morningmedley/hook",
    "description": "A system for organizing WordPress hooks",
    "keywords": [
        "wordpress",
        "morningmedley",
        "hook"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Munk",
            "email": "mh@morningtrain.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "MorningMedley\\Hook\\": "src/",
            "MorningMedley\\Facades\\": "facades/"
        }
    },
    "require": {
        "php": "^8.2",
        "morningmedley/application": "^0.4"
    },
    "extra": {
        "morningmedley": {
            "providers": [
                "MorningMedley\\Hook\\HookServiceProvider"
            ]
        }
    }
}
